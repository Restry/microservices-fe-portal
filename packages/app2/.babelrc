{
    "presets": [
        [
            "@babel/preset-env", 
            {
                "modules": false,
                "targets": {
                    "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
                }
            }
        ]
    ],
    "plugins": [
        // 解析vue的jsx语法
        "transform-vue-jsx",
        "syntax-jsx",
        "@babel/plugin-transform-runtime",
        "@babel/plugin-syntax-dynamic-import"
    ]
}